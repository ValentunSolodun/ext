{
  "name": "XHRinfo",
  "description": "Get presentable info about network log resources",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "XHRinfo"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-B4tNbrv4DH8epxAc77lXQzb8jqeUkuqwoNLxawppUyw='; object-src 'self'",
  "permissions": [
    "debugger"
  ],
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"]
  }
}